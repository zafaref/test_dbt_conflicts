# Default is the main prefect repo
Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/de-prefect-build-key

# All other repos need their own hostname
Host de-dbt.github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/de-dbt-build-key

